{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "webworker"],
    "outDir": "dist",
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "preserveConstEnums": true
  },
  "include": [
    "./src/*.ts",
    "./node_modules/@cloudflare/workers-types/*.ts"
  ]
}
